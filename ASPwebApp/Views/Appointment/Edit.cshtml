@model Appointment

<h2>Edit Appointments</h2>
<hr>
<form asp-controller="Appointment" asp-action="Edit" method="post">
    <input type="hidden" asp-for="AppId">
    <div class="mb-3">
        <label asp-for="Title" class="form-label">Title</label>
        <input asp-for="Title" class="form-control">
    </div>
    <button type="submit" class="btn btn-secondary mt-2">Save</button>

</form>